{
  "name": "quack",
  "version": "1.0.0",
  "description": "A word guessing game with a ducky element.",
  "scripts": {
    "prestart" : "npm run tsc; npm run sass",
    "start" : "node ./build/app.js",
    "dev": "npm run prestart; npm run tsc:dev & npm run sass:dev & nodemon ./build/app.js",
    "tsc": "tsc -p tsconfig.server.json; tsc -p tsconfig.client.json",
    "tsc:dev": "tsc -p tsconfig.server.json -w & tsc -p tsconfig.client.json -w",
    "sass": "node-sass src/ -o build/public/",
    "sass:dev": "node-sass -q -r -w src/ -o build/public/",
    "clean": "rm -rf build/; mkdir build"
  },
  "author": "Lauren Zou",
  "engines": {
    "node": "9.10.1"
  },
  "main" : "./build/app.js",
  "dependencies": {
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node-sass-middleware": "0.0.31",
    "nodemon": "^2.0.1"
  }
}
